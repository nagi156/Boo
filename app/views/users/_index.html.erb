<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th scopre="col">image</th>
      <th scopre="col">name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image, size: "80x80" %></td>
        <td><%= user.name %></td>
        <td>
          <%= link_to user_path(user) do %>
           Show
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>